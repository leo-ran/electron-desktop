{
    "parser":"babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "shared-node-browser": true,
        "es6":true
    },
    "plugins":[
        "html"
    ],
    "rules": {
        "quotes":[1,"single"],
        "eqeqeq": 2,
        "no-extra-semi":2,
        "no-duplicate-case":2,
        "no-extra-parens":2,
        "no-extra-boolean-cast":2,
        "no-unused-vars": 2,
        "no-dupe-keys":2,
        "no-empty-function":2,
        "no-empty-pattern":2,
        "no-redeclare":2,
        "no-self-assign":2,
        "no-useless-escape":2,
        "no-mixed-spaces-and-tabs":2,
        "constructor-super":2,
        "no-class-assign":2,
        "no-const-assign":2,
        "no-dupe-class-members":2,
        "no-this-before-super":2,
        "comma-spacing":2,
        "space-infix-ops": 2,
        "keyword-spacing":2,
        "block-spacing":2
    }
}
{
  "name": "electron-desktop-ts-react",
  "version": "1.0.0",
  "main": "./dist/electron/main.js",
  "author": "YunlongRan",
  "license": "MIT",
  "description": "test",
  "scripts": {
    "start": "npm run dev",
    "dev": "node ./.lemonc/dev",
    "build": "node ./.lemonc/build && electron-builder",
    "dist": "node ./.lemonc/build"
  },
  "build": {
    "productName": "lemonc",
    "appId": "org.lemonc.app",
    "directories": {
      "output": "build"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "createDesktopShortcut": true,
      "multiLanguageInstaller": true,
      "menuCategory": true,
      "license": "./license.txt"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/electron/**/*"
    ],
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "linux": {
      "icon": "build/icons"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "@babel/plugin-transform-runtime": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-react": "^7.0.0-rc.1",
    "@babel/preset-typescript": "^7.0.0-rc.1",
    "@babel/runtime": "^7.0.0-rc.1",
    "autoprefixer": "^9.1.1",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "cfonts": "^2.1.3",
    "chalk": "^2.4.1",
    "css-loader": "^1.0.0",
    "electron": "^2.0.5",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "ora": "^3.0.0",
    "postcss": "^7.0.2",
    "postcss-loader": "^3.0.0",
    "precss": "^3.1.2",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.4.2",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.0.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.3",
    "webpack-dev-server": "^3.1.5",
    "webpack-hot-middleware": "^2.22.3"
  },
  "dependencies": {
    "@types/node": "^10.5.8",
    "@types/react": "^16.4.10",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.6",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  }
}

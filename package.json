{
  "name": "electron-desktop",
  "version": "1.0.1",
  "main": "./dist/electron/main.js",
  "author": "YunlongRan",
  "license": "MIT",
  "description": "test",
  "scripts": {
    "start": "npm run dev",
    "dev": "node ./.lemonc/dev",
    "build": "node ./.lemonc/build && electron-builder",
    "dist": "node ./.lemonc/build"
  },
  "build": {
    "productName": "lemonc",
    "appId": "org.lemonc.app",
    "directories": {
      "output": "build"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "createDesktopShortcut": true,
      "multiLanguageInstaller": true,
      "menuCategory": true,
      "license": "./license.txt"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/electron/**/*"
    ],
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "linux": {
      "icon": "build/icons"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "cfonts": "^2.1.3",
    "chalk": "^2.4.1",
    "css-loader": "^1.0.0",
    "electron": "^2.0.5",
    "eslint": "^5.2.0",
    "eslint-loader": "^2.1.0",
    "file-loader": "^1.1.11",
    "fonts": "^0.0.2",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "ora": "^3.0.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.2.6",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.16.3",
    "webpack-dev-server": "^3.1.5",
    "webpack-hot-middleware": "^2.22.3"
  },
  "dependencies": {
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  }
}
